{
  "name": "medical-backend",
  "version": "1.0.0",
  "description": "Comprehensive backend for medical application with AI prediction and real-time features",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "create-admin": "node scripts/create-admin.js",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "docker:build": "docker build -t medical-backend .",
    "docker:run": "docker run -p 5000:5000 medical-backend",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "generate:postman": "node scripts/generate-postman.js",
    "test:api": "node scripts/test-api.js",
    "seed:admin": "node utils/seedAdmin.js"
  },
  "keywords": [
    "express",
    "mongodb",
    "medical",
    "ai",
    "prediction",
    "websocket",
    "real-time",
    "healthcare"
  ],
  "author": "Medical App Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.7",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/medical-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/medical-backend/issues"
  },
  "homepage": "https://github.com/your-org/medical-backend#readme"
}
